{
  "name": "prefix-property",
  "version": "1.0.0",
  "description": "Get the vendor-prefixed property.",
  "main": "dist/prefix-property.js",
  "scripts": {
    "start": "npm run test",
    "test": "npm run lint && npm run build && npm run test-node && npm run test-browser",
    "test-browser": "open test/test.html",
    "test-node": "./node_modules/.bin/mocha test/test.js",
    "test-watch": "./node_modules/.bin/mocha test/test --watch",
    "build": "npm run babel && npm run browserify && npm run uglify",
    "babel": "node_modules/.bin/babel src/prefix-property.js -o dist/prefix-property.js",
    "browserify": "node_modules/.bin/browserify src/prefix-property.js > dist/prefix-property.browser.js -t babelify -s prefixProperty && node_modules/.bin/browserify test/test.js > test/test.browser.js -t babelify",
    "lint": "node_modules/.bin/eslint src/**/*.js test/test.js",
    "uglify": "node_modules/.bin/uglifyjs dist/prefix-property.browser.js -c -m -o dist/prefix-property.browser.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jshanson7/prefix-property.git"
  },
  "author": "Jeff Hanson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jshanson7/prefix-property/issues"
  },
  "homepage": "https://github.com/jshanson7/prefix-property#readme",
  "dependencies": {
    "lodash": "^3.10.1"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.3",
    "babelify": "^6.3.0",
    "browserify": "^11.2.0",
    "eslint": "^1.6.0",
    "mocha": "^2.3.3",
    "uglifyjs": "^2.4.10"
  }
}
